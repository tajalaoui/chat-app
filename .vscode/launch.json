{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "skipFiles": ["<node-internals>/**"],
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      // "program": "${workspaceFolder}/server/index.js"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: dev",
      "request": "launch",
      "command": "yarn dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}
